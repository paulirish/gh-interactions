{
  "name": "gh-interactions",
  "version": "1.0.0",
  "minimum_chrome_version": "72",
  "manifest_version": 2,
  "description": "gh interactions is cool",
  "background": {
    "scripts": [
      "scripts/GitHub.bundle.js",
      "scripts/background-entry.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "unlimitedStorage",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'none'"
}
